        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:HumanGeneratedNumbers"
        mc:Ignorable="d"
        Title="MainWindow" Height="630" Width="911">
    <Grid>
        <TabControl x:Name="MyTabControl">
            <TabItem Header="Welcome!">
                <Grid>
                    <TextBox x:Name="NumberTextBox" HorizontalAlignment="Left" Margin="188,164,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="120"/>
                    <Button Content="Get Occurences" Cursor="Hand" Click="Button_Click" Margin="335,145,0,0" Height="58" VerticalAlignment="Top" HorizontalAlignment="Left" Width="100"/>
                    <Label x:Name="NumLabel" Content="Label" HorizontalAlignment="Left" Margin="468,164,0,0" VerticalAlignment="Top"/>
                </Grid>
            </TabItem>
        </TabControl>
    </Grid>
</Window>
